{
  "@context": "http://qiicr.org/dcmqi/contexts/dcmqi.jsonld",
  "id" : "http://qiicr.org/schema/dcmqi/common-schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",

  "definitions" : {
    "PN" : {
      "type" : "string",
      "maxLength" : 64
    },
    "CS" : {
      "type" : "string",
      "maxLength" : 16
    },
    "IS" : {
      "type" : "string",
      "maxLength" : 12
    },
    "LO" : {
      "type" : "string",
      "maxLength" : 64
    },
    "SH" : {
      "type" : "string",
      "maxLength" : 64
    },
    "codeSequence" : {
      "type" : "object",
      "properties" : {
        "CodeValue" : { "type" : "SH", "default" : "T-D0050"},
        "CodingSchemeDesignator" : { "type":"SH", "default" : "SRT"},
        "CodeMeaning" : { "type":"LO", "default" : "Tissue"}
      },
      "required" : ["CodeValue","CodingSchemeDesignator","CodeMeaning"]
    },

    "dicomInstancesFileNameList" : {
      "type" : "array",
      "items" : "string"
    },

    "compositeContext" : {
      "type" : "object",
      "properties" : {

        "iodPatientModule" : {
          "type" : "object",
          "properties" : {
            "PatientName" : {
              "type" : "string",
              "default" : ""
            },
            "PatientID" : {
              "type" : "string",
              "default" : ""
            },
            "PatientBirthDate" : {
              "type" : "string",
              "default" : ""
            },
            "PatientSex" : {
              "type" : "string",
              "default" : ""
            }
          }
        },

        "iodGeneralStudyModule" : {
          "type" : "object",
          "properties" : {
            "StudyInstanceUID" : {
              "type" : "UI",
              "default" : "1.1.1.1"
            },
            "StudyDate" : {
              "type" : "DA",
              "default" : ""
            },
            "StudyTime" : {
              "type" : "TA",
              "default" : ""
            },
            "ReferringPhysicianName" : {
              "type" : "PN",
              "default" : ""
            },
            "StudyID" : {
              "type" : "string",
              "default" : ""
            },
            "AccessionNumber" : {
              "type" : "string",
              "default" : ""
            }
          }
        },

        "iodGeneralEquipmentModule" : {
          "type" : "object",
          "properties" : {
            "Manufacturer" : {
              "type" : "string",
              "default" : ""
            }
          }
        },

        "iodGeneralSeriesModule" : {
          "type" : "object",
          "properties" : {
            "Modality" : {
              "type" : "string",
              "default" : "MR"
            },
            "SeriesInstanceUID" : {
              "type" : "UI",
              "default" : "2.2.2.2"
            }
          }
        },

        "iodFrameOfReferenceModule" : {
          "type" : "object",
          "properties" : {
            "FrameOfReferenceUID" : {
              "type" : "UI",
              "default" : "3.3.3.3"
            }
          }
        }
      }
    }
  }
}
